            env.update({
                'PORT': str(frontend_port),
                'HOST': '0.0.0.0',
                'NODE_OPTIONS': '--max-old-space-size=4096',  # Increase memory
                'FORCE_COLOR': '0',  # Avoid color codes that cause encoding issues
